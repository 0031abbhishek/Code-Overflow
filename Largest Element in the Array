import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int arr[] = {1,4,9,3,22,3,-5,6};
		int len = arr.length;
		int max=Integer.MIN_VALUE;
		for(int i=0;i<len;i++){
		    if(arr[i]>max) max=arr[i];
		}
		System.out.println("Largest Element in the Array is "+max);
		
	}
}
