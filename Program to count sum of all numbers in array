//Program to count sum of all numbers in array
#include <iostream>
using namespace std;
int const length = 10;
int temp[length] = { 2,5,4,14,5,4,1,8,6,0 };
int sum = 0;
int main()
{
	for (int i = 0; i <= length; i++)
	{
		sum = temp[i]+ sum;
    }
	cout << "Sum of all elements of the array is: " << sum << endl;
	system("pause");
}

